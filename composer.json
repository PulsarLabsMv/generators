{
    "name": "abunooh/generators",
    "description": "A powerful and extendable tool for generating code in Laravel applications",
    "homepage": "https://github.com/Abunooh/generators",
    "license": "MIT",
    "keywords": [
        "abunooh",
        "laravel",
        "generators"
    ],
    "authors": [
        {
            "name": "Hussain Afeef",
            "email": "ibnahnajjaar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^10.0 || ^11.0",
        "doctrine/dbal": "^4.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^9.5 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "Abunooh\\Generators\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Abunooh\\Generators\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Abunooh\\Generators\\GeneratorsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
